{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@assets/*":[
        "./src/assets/*"
      ],
      "@components/*":[
        "src/components/*"
      ],
      "@routes/*":[
        "./src/routes/*"
      ],
      "@screens/*":[
        "./src/screens/*"
      ],
      "@storage/*":[
        "./src/storage/*"
      ],
      "@utils/*":[
        "./src/utils/*"
      ],
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
